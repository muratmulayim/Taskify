<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   	<extension id="product"
		point="org.eclipse.core.runtime.products">
      	<product
			name="Product"
            application="org.eclipse.ui.ide.workbench">
         <property
			name="appName"
            value="Product">
         </property>
		</product>
	</extension>
	
   	<extension id="product"
		point="org.eclipse.core.runtime.products">
		<product
			application="org.eclipse.ant.core.antRunner"
			name="DSLDeveloper">
			<property
				name="aboutText"
				value="DSL Developer by Kim">
			</property>
			<property
				name="aboutImage"
				value="/org.xtext.example.mydsl.product/icons/dsl_run.gif">
			</property>
         	<property
               	name="startupForegroundColor"
               	value="51f544">
         	</property>
         	<property
               	name="startupProgressRect"
               	value="2,290,448,10">
         	</property>
         	<property
               	name="startupMessageRect"
               	value="7,265,320,20">
         	</property>
         	<property
               	name="preferenceCustomization"
               	value="plugin_customization.ini">
         	</property>
         	<property
               	name="appName"
               	value="DSLDeveloper">
         	</property>
      	</product>
   	</extension>
</plugin>
