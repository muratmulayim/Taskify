<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="myDsl" nsURI="http://www.xtext.org/example/mydsl/MyDsl" nsPrefix="myDsl">
  <eClassifiers xsi:type="ecore:EClass" name="InkApp">
    <eStructuralFeatures xsi:type="ecore:EReference" name="globals" upperBound="-1"
        eType="#//GlobalVariableExpression" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="tasks" upperBound="-1"
        eType="#//Task" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="entry" eType="#//EntryTask"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BreakExpression" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="break" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableType">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GlobalVariableExpression" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="variable" eType="#//VariableDeclerationExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EntryTask">
    <eStructuralFeatures xsi:type="ecore:EReference" name="task" eType="#//Task" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Task">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="taskbody" eType="#//TaskBody"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IfExpression" eSuperTypes="#//BlockExpressions #//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ifcondition" eType="#//OperationExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ifbody" eType="#//StatementBody"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="elseifcondition" upperBound="-1"
        eType="#//OperationExpression" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="elseifbody" upperBound="-1"
        eType="#//StatementBody" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="elsebody" eType="#//StatementBody"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ForExpression" eSuperTypes="#//BlockExpressions #//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="initial" eType="#//OperationExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="test" eType="#//OperationExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="update" eType="#//OperationExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="body" eType="#//StatementBody"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WhileExpression" eSuperTypes="#//BlockExpressions #//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="test" eType="#//OperationExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="body" eType="#//StatementBody"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BuiltinFunctionCallExpression" eSuperTypes="#//BlockExpressions #//Expression">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="function" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
        eType="#//Atomic" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TaskBody">
    <eStructuralFeatures xsi:type="ecore:EReference" name="body" upperBound="-1" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nexttask" eType="#//Task"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="endtask" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StatementBody">
    <eStructuralFeatures xsi:type="ecore:EReference" name="body" upperBound="-1" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BlockExpressions"/>
  <eClassifiers xsi:type="ecore:EClass" name="Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="OperationExpression" eSuperTypes="#//BlockExpressions #//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="ArrayDimension">
    <eStructuralFeatures xsi:type="ecore:EReference" name="index" eType="#//VariableSymbol"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableDeclerationExpression" eSuperTypes="#//Expression #//VariableSymbol">
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//VariableType"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dimension" upperBound="-1"
        eType="#//ArrayDimension" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableSymbol"/>
  <eClassifiers xsi:type="ecore:EClass" name="Atomic" eSuperTypes="#//OperationExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="Operation" eSuperTypes="#//OperationExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Atomic"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" upperBound="-1"
        eType="#//Atomic" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IntegerReference" eSuperTypes="#//Atomic">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DoubleReference" eSuperTypes="#//Atomic">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StringReference" eSuperTypes="#//Atomic">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BooleanReference" eSuperTypes="#//Atomic">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ArrayReference" eSuperTypes="#//Atomic">
    <eStructuralFeatures xsi:type="ecore:EReference" name="variableReference" eType="#//VariableSymbol"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dimension" upperBound="-1"
        eType="#//ArrayDimension" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableReference" eSuperTypes="#//Atomic">
    <eStructuralFeatures xsi:type="ecore:EReference" name="variableReference" eType="#//VariableSymbol"/>
  </eClassifiers>
</ecore:EPackage>
